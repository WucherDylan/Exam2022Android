<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Historique">
<ImageView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:src="@drawable/ike"
    android:scaleType="center"
    />
<GridLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:columnCount="2">


    <TextView
        android:id="@+id/titre1"
        android:layout_width="187dp"
        android:layout_gravity="left"
        android:textSize="@dimen/texte"
        android:text="Siret :"
        android:fontFamily="@string/font_family_thin"/>

    <TextView
        android:id="@+id/siren"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@string/font_family_thin"
        android:text="N/a"
        android:textSize="@dimen/texte" />

    <TextView
        android:id="@+id/titre2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@string/font_family_thin"
        android:textSize="@dimen/texte"
        android:text="Siren :" />

    <TextView
        android:id="@+id/siret"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@string/font_family_thin"
        android:textSize="@dimen/texte"
        android:text="N/a" />

    <TextView
        android:id="@+id/nomSociete"
        android:layout_width="fill_parent"
        android:fontFamily="@string/font_family_thin"
        android:layout_height="43dp"
        android:layout_columnSpan="02"
        android:layout_marginTop="10sp"
        android:layout_marginBottom="0sp"
        android:text="N/a"
        android:textAlignment="center"
        android:textSize="@dimen/texte" />

    <TextView
        android:id="@+id/ActivitePrincipal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@string/font_family_thin"
        android:textSize="15sp"
        android:layout_columnSpan="2"
        android:layout_marginBottom="10sp"
        android:text="N/a" />
    <TextView
        android:id="@+id/titreRegion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@string/font_family_thin"
        android:textSize="@dimen/texte"
        android:text="Region :" />

    <TextView
        android:id="@+id/Region"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@string/font_family_thin"
        android:textSize="@dimen/texte"
        android:text="N/a" />
    <TextView
        android:id="@+id/Titredepartement"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@string/font_family_thin"
        android:textSize="@dimen/texte"
        android:text="DÃ©partement :" />

    <TextView
        android:id="@+id/dep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@string/font_family_thin"
        android:textSize="@dimen/texte"
        android:text="N/a" />
    <TextView
        android:id="@+id/TitreCodePostal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@string/font_family_thin"
        android:textSize="@dimen/texte"
        android:text="Code postal :" />

    <TextView
        android:id="@+id/CodePostal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@string/font_family_thin"
        android:textSize="@dimen/texte"
        android:text="N/a" />
    <TextView
        android:id="@+id/TitreEffectif"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@string/font_family_thin"
        android:textSize="@dimen/texte"
        android:text="Effectif :" />

    <TextView
        android:id="@+id/Effectif"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@string/font_family_thin"
        android:textSize="@dimen/texte"
        android:text="N/a" />

    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="400dp"
        android:layout_columnSpan="2"
        android:fontFamily="@string/font_family_thin"
        android:layout_below="@id/listSirene"
        />

</GridLayout>
</RelativeLayout>